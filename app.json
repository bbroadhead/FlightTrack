{
  "expo": {
    "name": "FlightTrack",
    "slug": "flighttrack",
    "scheme": "flighttrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "plugins": ["expo-router"],
    "experiments": {
      "baseUrl": "/FlightTrack",
      "typedRoutes": true
    }
  }
}
